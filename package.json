{
  "name": "superlink-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:backup": "node scripts/backup-database.js",
    "db:migrate": "node scripts/run-migration.js",
    "db:rollback": "node scripts/rollback-migration.js",
    "db:verify": "node scripts/verify-database.js",
    "dev": "next dev",
    "docker:build": "docker build -t superlink:latest .",
    "docker:run": "docker run -p 3000:3000 superlink:latest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security:audit": "npm audit --audit-level high",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "latest",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/auth-helpers-nextjs": "latest",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "latest",
    "framer-motion": "latest",
    "geist": "latest",
    "ioredis": "latest",
    "lucide-react": "^0.454.0",
    "next": "^15.2.4",
    "path-to-regexp": "^8.3.0",
    "picomatch": "^4.0.3",
    "qrcode": "latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "latest",
    "sonner": "^2.0.7",
    "source-map-js": "^1.2.1",
    "speakeasy": "latest",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "postcss-import": "16.1.1",
    "postcss-nesting": "12.1.1",
    "prettier": "3.1.0",
    "strip-ansi": "^7.1.2",
    "tailwindcss": "3.4.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
